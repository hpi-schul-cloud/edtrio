{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "variable-name": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-empty": false
  },
  "linterOptions": {
    "exclude": ["*.json", "**/*.json"]
  },
  "defaultSeverity": "error"
}
